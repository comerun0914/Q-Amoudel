{
  "public": ".",
  "cleanUrls": false,
  "trailingSlash": false,
  "headers": [
    {
      "source": "**/*.@(js|css)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/questionnaire-preview",
      "destination": "/questionnaire-preview.html"
    }
  ]
} 